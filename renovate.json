{
  "extends": ["config:base", "group:allNonMajor"],
  "labels": ["type: dependencies"],
  "vulnerabilityAlerts": {
    "labels": ["type: security"]
  },
  "rollbackPrs": true,
  "reviewersFromCodeOwners": true,
  "prHourlyLimit": 10,
  "lockFileMaintenance": { "enabled": true },
  "packageRules": [
    {
      "matchUpdateTypes": ["lockFileMaintenance", "major", "minor", "patch", "pin", "digest"],
      "automerge": true
    }
  ]
}
